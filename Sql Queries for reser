CREATE TABLE Seat (
  seatId INT NOT NULL PRIMARY KEY,
  seatNo VARCHAR(50) NOT NULL
);
CREATE TABLE reserved_seats (
  BusId INT NOT NULL,
  seatId INT NOT NULL,
  date DATE NOT NULL,
  status VARCHAR(50),
  bookedStatus VARCHAR(50),
  PRIMARY KEY (BusId, seatId, date),
  FOREIGN KEY (BusId) REFERENCES bus(BusId),
  FOREIGN KEY (seatId) REFERENCES seat(seatId)
);


