SELECT 
    a.date, a.BusId, a.seatId, a.status, a.bookedStatus,b.seatNo
FROM
    reserved_seats a inner join seat b on a.seatId = b.seatId WHERE
    a.BusId = 1 AND a.date = '2023-05-11';
    
